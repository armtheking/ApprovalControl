<%@ page contentType="text/html;charset=UTF-8" language="java" trimDirectiveWhitespaces="true" %><%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %><%@ taglib prefix="spring" uri="http://www.springframework.org/tags" %><%@ taglib prefix="form" uri="http://www.springframework.org/tags/form" %><%@ taglib prefix="sec" uri="http://www.springframework.org/security/tags" %><!DOCTYPE html><html>    <head>        <%@ include file="../template/head.jspf" %>    </head>    <body>        <%@ include file="../template/pageTitle.jspf" %>        <div class="container">            <form:form id="form1" class="form-horizontal" enctype="multipart/form-data"                       method="post" action="success?${_csrf.parameterName}=${_csrf.token}" >                <fieldset>                    <legend><i class="fa fa-book"></i> Training</legend>                </fieldset>                </br>                <div class="panel panel-info" style="width: 100%; margin: auto">                    <div class="panel-heading">                        <h3 class="panel-title"><b>Preview</b></h3>                    </div>                    <div class="panel-body">                        <div class="row" style="margin: 0.5%;">                               <div class="form-group form-group-sm">                                <div class="col-sm-3">                                    <b>Type of Training:</b>                                 </div>                                <div class="col-sm-3">                                    <mark>${ticketHTraining.trainingType.typeDescription} (${ticketHTraining.trainingPlan.planDescription})</mark>                                </div>                                <div class="col-sm-3">                                    <b>Total Persons:</b>                                 </div>                                <div class="col-sm-3">                                    <mark>${ticketHTraining.totalPerson}</mark>                                </div>                            </div>                        </div>                        <div class="row" style="margin: 0.5%;">                             <div class="form-group form-group-sm">                                <div class="col-sm-3">                                    <b>Training Title:</b>                                 </div>                                <div class="col-sm-3">                                    <mark>${ticketHTraining.ticketHeader.detailHeader}</mark>                                </div>                            </div>                        </div>                        <div class="row" style="margin: 0.5%;">                             <div class="form-group form-group-sm">                                <div class="col-sm-3">                                    <b>Objective:</b>                                 </div>                                <div class="col-sm-9">                                    <mark>${ticketHTraining.ticketHeader.remarkHeader}</mark>                                </div>                            </div>                        </div>                        <div class="row" style="margin: 0.5%;">                            <div class="form-group form-group-sm">                                <div class="col-sm-3">                                    <b>Start Date:</b>                                </div>                                <div class="col-sm-3">                                    <mark>${ticketHTraining.startDate}</mark>                                </div>                                <div class="col-sm-3">                                    <b>End Date:</b>                                 </div>                                <div class="col-sm-3">                                    <mark>${ticketHTraining.endDate}</mark>                                </div>                            </div>                        </div>                        <div class="row" style="margin: 0.5%;">                            <div class="form-group form-group-sm">                                <div class="col-sm-3">                                    <b>Place:</b>                                 </div>                                <div class="col-sm-3">                                    <mark>${ticketHTraining.place}</mark>                                </div>                                <div class="col-sm-3">                                    <b>Organized by:</b>                                 </div>                                <div class="col-sm-3">                                    <mark>${ticketHTraining.organizeBy}</mark>                                </div>                            </div>                        </div>                        <div class="row" style="margin: 0.5%;">                             <div class="form-group form-group-sm">                                <div class="col-sm-3">                                    <b>Payment:</b>                                 </div>                                <div class="col-sm-3">                                    <mark>${ticketHTraining.trainingPayment.paymentDescription}</mark>                                </div>                                <div class="col-sm-3">                                    <b>Date of Payment:</b>                                 </div>                                <div class="col-sm-3">                                    <mark>${ticketHTraining.paymentDate}</mark>                                </div>                            </div>                        </div>                        <div class="row" style="margin: 0.5%;">                             <div class="form-group form-group-sm">                                <div class="col-sm-3">                                    <b>Content of Training:</b>                                 </div>                                <div class="col-sm-3">                                    <a href="<c:url value='/downloadTraining?id=${principal.username}' />" target="_blank">                                        <img border="0" alt="${principal.username}" src="<c:url value='/resources/img/pdf.png' />" width="30" height="30">                                    </a>                                </div>                                <div class="col-sm-3">                                    <b>List of Participant:</b>                                </div>                                 <div class="col-sm-3">                                    <a href="<c:url value='/downloadTraining?id=${principal.username}-Participant' />" target="_blank">                                        <img border="0" alt="${principal.username}" src="<c:url value='/resources/img/pdf.png' />" width="30" height="30">                                    </a>                                </div>                            </div>                        </div>                    </div>                                                        <div class="row" style="margin: 0.5%;">                         <table id="tb_participant"  class="table table-bordered" cellspacing="0" width="100%">                            <thead>                                <tr class="danger">                                    <th>No</th>                                                                   <th>EmployeeID</th>                                    <th>Firstname</th>                                    <th>Lastname</th>                                    <th>Section</th>                                </tr>                            </thead>                            <tbody>                                   <c:forEach varStatus="varStatus" var="trainingParticipant" items="${ticketHTraining.ticketHeader.trainingParticipant}">                                    <tr>                                             <c:if test="${trainingParticipant.empID != ''}">                                            <td>${varStatus.count}</td>                                            <td>${trainingParticipant.empID}</td>                                            <td>${trainingParticipant.firstName}</td>                                            <td>${trainingParticipant.lastName}</td>                                            <td>${trainingParticipant.section}</td>                                        </c:if>                                        <c:if test="${trainingParticipant.empID == ''}">                                            <td>-</td>                                            <td>-</td>                                            <td>-</td>                                            <td>-</td>                                            <td>-</td>                                        </c:if>                                       </tr>                                </c:forEach>                            </tbody>                        </table>                    </div>                    <div class="row" style="margin: 0.5%;">                         <table id="tb"  class="table table-bordered" cellspacing="0" width="100%">                            <thead>                                <tr class="success">                                    <th class="col-md-1">No</th>                                                                   <th class="col-md-3">Item</th>                                    <th class="col-md-6">Details</th>                                    <th class="col-md-2">Amount</th>                                </tr>                            </thead>                            <tbody>                                   <c:forEach varStatus="varStatus" var="ticketDTraining" items="${ticketDTraining}">                                    <tr>                                        <c:if test="${ticketDTraining.amount != null}">                                            <td>${varStatus.count}</td>                                            <td>${ticketDTraining.item}</td>                                             <td style="max-width: 360px; word-wrap: break-word;">${ticketDTraining.budgetDetail}</td>                                                                       <td align="right">${ticketDTraining.amount}</td>                                        </c:if>                                    </tr>                                </c:forEach>                            </tbody>                        </table>                        <div style="margin-left: 82%"><font color="red"> <u>  Total Amount:  ${number_sumAmount}  Cost/Head: ${number_costPerHead}</u></font></div>                    </div>                    <center>                        <input type="submit" class="btn btn-success btn-lg" value="Submit" name="submit" />                    </center>                </div>            </form:form>        </div>        <%@ include file="../template/footer.jspf" %>        <%@ include file="../template/scripts.jspf" %>    </body></html>